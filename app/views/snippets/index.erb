<div class="jumbotron jumbotron-fluid">
  <div class="container">
    <h1>All Snippets</h1>
    <p>Here is the current list of All Snippets.</p>
    <% @snippets = Snippet.all  %>
    <% @snippets.each do |snippet| %>
      <div>
        Snippet Category: <%=snippet.snippet_category%>
        <br>
        Snippet Name: <%=snippet.snippet_name%>
        <br>
        Snippet Text: <%=snippet.snippet_text%>
      </div>
    <% end %>
   </div>
 </div>
