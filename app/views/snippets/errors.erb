<div class="jumbotron jumbotron-fluid">
  <div class="container">
    <h2>Create A New Snippet Errors</h2>
    <p>Here is the list of errors. Please correct your errors.</p>

    <% if @snippet.errors.any? %>
      <div id="error_explanation">
        <ul>
        <% @snippet.errors.full_messages.each do |msg| %>
          <li><%= msg %></li>
        <% end %>
        </ul>
      </div>
    <% end %>
<a href="/snippets/new"><button class="btn btn-primary">GO BACK</button></a>
</div>
</div>
